{
  "name": "v2-from-scratch",
  "version": "0.1.0",
  "description": "Gatsby v2 MDX Tutorial",
  "main": "index.js",
  "author": "Andre Roussakoff <madrus@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "precommit": "pretty-quick --staged",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "rm -rf public && rm -rf .cache",
    "format-output": "prettier src/**/*.{js,jsx}",
    "format": "prettier --write src/**/*.{js,jsx}",
    "lint-errors": "eslint src/**/*.{js,jsx} --quiet",
    "lint": "eslint src/**/*.{js,jsx}"
  },
  "dependencies": {
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "@mdx-js/mdx": "^0.16.8",
    "@mdx-js/tag": "^0.16.8",
    "emotion-theming": "^10.0.7",
    "gatsby": "^2.0.111",
    "gatsby-image": "^2.0.29",
    "gatsby-mdx": "^0.3.5",
    "gatsby-plugin-catch-links": "^2.0.10",
    "gatsby-plugin-emotion": "^4.0.3",
    "gatsby-plugin-sharp": "^2.0.20",
    "gatsby-plugin-typography": "^2.2.7",
    "gatsby-remark-images": "^3.0.3",
    "gatsby-source-filesystem": "^2.0.20",
    "gatsby-transformer-remark": "^2.2.3",
    "gatsby-transformer-sharp": "^2.1.13",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-typography": "^0.16.18",
    "typeface-candal": "^0.0.54",
    "typeface-open-sans": "^0.0.54",
    "typography": "^0.16.18"
  },
  "devDependencies": {
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "prettier": "^1.16.3",
    "pretty-quick": "^1.10.0"
  },
  "remarkConfig": {
    "settings": {
      "emphasis": "_",
      "strong": "_"
    },
    "plugins": [
      "remark-lint-emphasis-marker",
      "remark-lint-strong-marker"
    ]
  }
}
